<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>Coding Moods</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <% if (locals.env === 'development') { %>
  <link href="/css/style.css" rel="stylesheet" />
  <link href="/components/prism/themes/prism.css" rel="stylesheet" />
  <% } else { %>
  <link href="/dist/beapy.min.css" rel="stylesheet" />
  <% } %>
</head>
<body>
  <div class="container">
    <section class="sidebar sidebar--clicked">
      <a class="toggle"></a>
      <%- include menu.ejs %>
    </section>
    <header>
      <h1 class="title">Coding Moods</h1>
      <p class="subtitle"><%= t.subtitle %><a href='https://github.com/jfroffice/beapy'>Beapy BETA</a>.</p>
    </header>
    <section class="content">
      <section class="article">
        <header class="current"></header>
        <article></article>
      </section>
    </section>
    <section class="comment">
      <div id="disqus_thread"></div>
      <a href="http://disqus.com" class="dsq-brlink"><%= t.disqus %><span class="logo-disqus">Disqus</span></a>
    </section>
  </div>
  <script src="http://cdnjs.cloudflare.com/ajax/libs/zepto/1.1.3/zepto.min.js"></script>
  <script>var LANG = "<%= lang %>";</script>
  <% if (env === 'development') { %>
  <script src="components/prism/prism.js"></script>
  <script src="js/main.js"></script>
  <% } else { %>
  <script src="dist/beapy.min.js"></script>
  <% } %>
  <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
  ga('create', 'UA-48734572-1', 'jfroffice.me');
  ga('send', 'pageview');
  </script>
</body>
</html>